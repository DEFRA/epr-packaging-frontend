@model FrontendSchemeRegistration.UI.Controllers.SignedInAccountViewModel

<div class="govuk-width-container">
    <main class="govuk-main-wrapper govuk-!-padding-top-4" id="main-content" role="main">
        <div class="govuk-grid-row">
            <div class="govuk-grid-column-two-thirds">
                <h1 class="govuk-heading-xl">You've signed in</h1>
                <p>Id @Model.Id</p>
                <p>Email @Model.Email</p>
                
                <p>
                    <a href="@Model.ReturnUrl" class="govuk-button">Continue</a>
                </p>
            </div>
        </div>
    </main>
</div>
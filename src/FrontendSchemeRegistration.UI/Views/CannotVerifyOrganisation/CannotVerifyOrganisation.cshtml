@{
    ViewData["Title"] = Localizer["CannotVerifyOrganisation.Title"];
}

@await Html.PartialAsync("Partials/Govuk/_AccountActionBar")

<div class="govuk-width-container">
    <main class="govuk-main-wrapper govuk-!-padding-top-4" id="main-content" role="main">
        <div class="govuk-grid-row">
            <div class="govuk-grid-column-two-thirds">
                <h1 class="govuk-heading-l govuk-!-margin-bottom-4">
                    @Localizer["CannotVerifyOrganisation.ContentHeading"]
                </h1>
                <p class="govuk-body">@Localizer["CannotVerifyOrganisation.Paragraph1"]</p>
                <p class="govuk-body">@Localizer["CannotVerifyOrganisation.Paragraph2"]</p>
            </div>
        </div>
    </main>
</div>